                                                            <option value="UPADHYAY">UPADHYAY</option>
                                                            <option value="VYAS">VYAS</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="form-group row">
                                                    <label class="col-md-2 col-form-label">Gotra</label>
                                                    <div class="col-md-10">
                                                        <select class="form-control" name="gotra" required>
                                                            <option value="" disabled selected>Select Gotra</option>
                                                            <option value="ANGRISH">ANGRISH</option>
                                                            <option value="ANKRISH">ANKRISH</option>
                                                            <option value="ATRI">ATRI</option>
                                                            <option value="AUDICHYA">AUDICHYA</option>
                                                            <option value="AUGUTSAY">AUGUTSAY</option>
                                                            <option value="BHARDWAJ">BHARDWAJ</option>
                                                            <option value="BHRIGU">BHRIGU</option>
