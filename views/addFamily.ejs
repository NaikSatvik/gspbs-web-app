                                                            <option value="GORAKHPUR">GORAKHPUR</option>
                                                            <option value="GORAKHPUR UP">GORAKHPUR UP</option>
                                                            <option value="GUJARAT">GUJARAT</option>
                                                            <option value="HMT">HMT</option>
                                                            <option value="INDORE">INDORE</option>
                                                            <option value="JAUNPUR">JAUNPUR</option>
                                                            <option value="KANPUR">KANPUR</option>
                                                            <option value="KUSHINAGAR">KUSHINAGAR</option>
                                                            <option value="MIRZAPUR">MIRZAPUR</option>
                                                            <option value="MP">MP</option>
                                                            <option value="MUJZAFARPUR">MUJZAFARPUR</option>
                                                            <option value="NATHDWARA">NATHDWARA</option>
                                                            <option value="PANNA (MP)">PANNA (MP)</option>
                                                            <option value="PRATAPGADHA">PRATAPGADHA</option>
                                                            <option value="RAIBARELI">RAIBARELI</option>
                                                            <option value="REWA">REWA</option>
                                                            <option value="SAMHO ITAWA">SAMHO ITAWA</option>
                                                            <option value="SANT KABIR NAGAR">SANT KABIR NAGAR</option>
                                                            <option value="SIDDHARTHNAGAR">SIDDHARTHNAGAR</option>
